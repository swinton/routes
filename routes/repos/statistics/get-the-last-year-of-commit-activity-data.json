      "body": [
        {
          "days": [
            0,
            3,
            26,
            20,
            39,
            1,
            0
          ],
          "total": 89,
          "week": 1336280400
        }
      ]
    }
  ],
  "idName": "get-commit-activity-stats",
  "documentationUrl": "https://developer.github.com/enterprise/2.12/v3/repos/statistics/#get-the-last-year-of-commit-activity-data"
